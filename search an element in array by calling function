from numpy import*
from array import*
def find_array(n):
    k=0
    for e in arr:
        if e==n:
            print("Index value u searched for is in")
            print(k)
            break
        k+=1
    else:
         print("Enter the correct element")
arr=array('i',[])
num=int(input("Enter the length of array"))
for i in range(num):
l=int(input("Enter the  values of the array"))
arr.append(l)
print(arr)
n=int(input("Enter the number to search"))
find_array(n)
       
       
output:
