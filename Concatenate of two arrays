from numpy import*
from array import*
arr=array('i',[])
num=int(input("Enter the length of array 1"))
for i in range(num):
    l=int(input("Enter the next value"))
    arr.append(l)
    print(arr)
    
arr2=array('i',[])
num2=int(input("Enter the length of array 2"))
for i in range(num2):
    k=int(input("Enter the  value of array"))
    arr.append(k)
    print(arr2)
print("Concatinated array is")
print(concatenate([arr,arr2]))
       
